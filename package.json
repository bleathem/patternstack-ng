{
  "name": "patternstack-ng",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ng serve",
    "serve:docker": "ng serve --host 0.0.0.0 --port 4200 --disable-host-check",
    "build": "ng build",
    "ghpages-build": "ng build --prod --deploy-url='/phoebe-ui' --base-href='/phoebe-ui/'",
    "ghpages-publish": "git -C dist init; git -C dist add .; git -C dist commit -m'dist'; git -C dist remote add bleathem git@github.com:bleathem/phoebe-ui.git; git -C dist push -f bleathem master:gh-pages",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "http-server dist/ -a 0.0.0.0 -p8080",
    "docker:build": "docker build -t patternstack/patternstack-ng .",
    "docker:dev": "docker run -p 4200:4200 -v ${PWD}/src:/usr/src/app/src -d patternstack/patternstack-ng npm run serve:docker"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "core-js": "^2.4.1",
    "patternfly": "^3.27.2",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.1",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
